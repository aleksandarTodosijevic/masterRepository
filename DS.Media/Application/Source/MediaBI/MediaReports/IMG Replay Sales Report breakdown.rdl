    <DataSource Name="MediaDM">
      <DataSourceReference>MediaDM</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>8d3f1d19-899a-4dec-a01a-b79dbb37734c</rd:DataSourceID>
