  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>MediaDM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@YearFilter">
            <Value>=Parameters!YearFilter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MonthInt">
            <Value>=Parameters!MonthInt.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
DECLARE @AreaOfBussiness varchar(10) = 'Archive'

IF OBJECT_ID('tempdb..#Properties') IS NOT NULL
	DROP TABLE #Properties

SELECT 
	p.[Property Key],
	p.[OPA Number], 
	p.[OPA Number Technical],
	p.[Accountant Responsible], 
	p.[Property Name], 
	p.[Area Of Business], 
	p.[Exec Responsible],
	p.[Sales Category (MDS)],
	RTRIM(p.[Abbreviation]) AS [Abbreviation],
	p.[Commission],
	p.[Commission INT],
	p.[Comment],
	COALESCE(op.OmitException, 0) AS OmitException
INTO #Properties
FROM [dbo].[Property] p
LEFT JOIN 
(
	SELECT 
		[Property Key], 1 AS OmitException
	FROM [dbo].[Property]
	WHERE [OPA Number] IN ('38829281','34601881','34601981','34602081','34602381','34602481','34602581','34733481','34733581','36292281','
									 36798381','37632281','37874981','38162581','38772481','39378181','39643881','39643981','39826381','44138781', 
									   '39826381') -- Ticket #161
) op ON p.[Property Key] = op.[Property Key]
WHERE 
	(
		p.[Area Of Business] LIKE @AreaOfBussiness
		AND p.[OPA Number Technical] NOT IN ('39662581','39834481','39841381','44042181','44433081') -- These were specified in tickets #126
	)
	OR -- OPA numbers from Comment no.5 from SOL ticket #87, #133,#126
	(
		p.[OPA Number] IN  
			('31651675',' 38391220',' 38391220',' 38993275') 
	)

CREATE CLUSTERED INDEX CI_PropertyKey ON #Properties ([Property Key])

SELECT 
	d.[Deal No],
	sa.[Sales Area],
	cc.[Customer Name],
	CAST(dd.[Date (DM Due Date)] AS DATE) AS [Date (DM Due Date)],
	i.[Is DM Due Date],
	aa.ProjectionYear,
	--a.AllocationsKey,
	p.[OPA Number], 
	p.[OPA Number Technical],
	--p.[OmitException],
	a.[Projection Name (Projection)] AS [Projection Name], 
	p.[Accountant Responsible], 
	a.[Projection No (Projection)] AS [Projection No],
	a.[Subject Category],
	p.[Property Name], 
	p.[Area Of Business], 
	p.[Exec Responsible], 
	p.[Sales Category (MDS)],
	p.[Abbreviation],
	a.[Fee Type],
	aa.ActualAmountUSD
,mf.[Actual Amount USD Filtered] AS [ActualAmountUSD chosen month]
FROM #Properties p
INNER JOIN 
(
	Select AllocationsKey,[Property Key], [Projection No (Projection)], [Projection Name (Projection)], [Subject Category], [Year], [Fee Type], SUM([Actual Amount USD]) AS [Actual Amount USD] from
	(
		-- Subquery for Amount for whole chosen year
		SELECT
			a.AllocationsKey,
			p.[Property Key],
			pr.[Projection No (Projection)],
			pr.[Projection Name (Projection)],
			pr.[Subject Category],
			ipd.[Calendar Year (DM Due Date)] AS [Year], 
			it.[Fee Type],
			a.[ActualAmountUSD] AS [Actual Amount USD]
		FROM #Properties p
		INNER JOIN [dbo].[Allocations] a ON p.[Property Key] = a.PropertyKey
		INNER JOIN [dbo].[DM Due Date] ipd ON a.DMDateKey = ipd.[DM Due Date Key]
		INNER JOIN [dbo].[Projection] pr ON a.ProjectionKey = pr.ProjectionKey
		INNER JOIN [dbo].[Income Type] it ON a.IncomeTypeKey = it.IncomeTypeKey
		WHERE ipd.[Calendar Year (DM Due Date)] = @YearFilter
			AND ipd.[Calendar Month Number Of Year (DM Due Date)] &lt;= @MonthInt
			AND a.ProjectionYear = @YearFilter

		UNION ALL
		-- Deal invoiced in previous years, but projection year is actual...
		SELECT
		a.AllocationsKey,
			p.[Property Key],
			pr.[Projection No (Projection)],
			pr.[Projection Name (Projection)],
			pr.[Subject Category],
			@YearFilter AS [Year], 
			it.[Fee Type],
			a.[ActualAmountUSD] AS [Actual Amount USD]
		FROM #Properties p
		INNER JOIN [dbo].[Allocations] a ON p.[Property Key] = a.PropertyKey
		INNER JOIN [dbo].[DM Due Date] ipd ON a.DMDateKey = ipd.[DM Due Date Key]
		INNER JOIN [dbo].[Projection] pr ON a.ProjectionKey = pr.ProjectionKey
		INNER JOIN [dbo].[Income Type] it ON a.IncomeTypeKey = it.IncomeTypeKey
		WHERE ipd.[Calendar Year (DM Due Date)] &lt; @YearFilter
			AND a.ProjectionYear = @YearFilter
	)as t
	GROUP BY AllocationsKey,[Property Key], [Projection No (Projection)], [Projection Name (Projection)], [Subject Category], [Year],[Fee Type]
) AS a ON p.[Property Key] = a.[Property Key]
LEFT JOIN
(
	SELECT
		AllocationsKey,[Property Key],
		[Year], 
		[Fee Type],
		SUM([ActualAmountUSD]) AS [Actual Amount USD Filtered]
		FROM
		(
			-- Subquery for Amount for chosen year and chosen month.
			SELECT
			a.AllocationsKey,
				p.[Property Key],
				ipd.[Calendar Year (DM Due Date)] AS [Year], 
				[Fee Type],
				a.[ActualAmountUSD]
			FROM #Properties p
			INNER JOIN [dbo].[Allocations] a ON p.[Property Key] = a.PropertyKey
			INNER JOIN [dbo].[DM Due Date] ipd ON a.DMDateKey = ipd.[DM Due Date Key]
			INNER JOIN [dbo].[Projection] pr ON a.ProjectionKey = pr.ProjectionKey
			INNER JOIN [dbo].[Income Type] it ON a.IncomeTypeKey = it.IncomeTypeKey
			WHERE ipd.[Calendar Year (DM Due Date)] = @YearFilter
				AND ipd.[Calendar Month Number Of Year (DM Due Date)] = @MonthInt
				AND a.ProjectionYear = @YearFilter

			UNION ALL
			-- Deal invoiced in previous years, but projection year is actual. Chosen month must be januar
			SELECT
			a.AllocationsKey,
				p.[Property Key],
				@YearFilter AS [Year], 
				[Fee Type],
				a.[ActualAmountUSD]
			FROM #Properties p
			INNER JOIN [dbo].[Allocations] a ON p.[Property Key] = a.PropertyKey
			INNER JOIN [dbo].[DM Due Date] ipd ON a.DMDateKey = ipd.[DM Due Date Key]
			INNER JOIN [dbo].[Projection] pr ON a.ProjectionKey = pr.ProjectionKey
			INNER JOIN [dbo].[Income Type] it ON a.IncomeTypeKey = it.IncomeTypeKey
			WHERE ipd.[Calendar Year (DM Due Date)] &lt; @YearFilter
				AND a.ProjectionYear = @YearFilter
				AND @MonthInt = 1
		) as t
		GROUP BY AllocationsKey,[Property Key], [Year], [Fee Type]
) AS mf ON p.[Property Key] = mf.[Property Key]
		AND a.[Year] = mf.[Year]
		AND a.[Fee Type] = mf.[Fee Type]
		AND a.AllocationsKey = mf.AllocationsKey

LEFT JOIN Allocations aa ON a.AllocationsKey = aa.AllocationsKey
LEFT JOIN Deal d ON aa.DealKey = d.[Deal Key]
LEFT JOIN [Sales Area] sa ON aa.SalesAreaKey = sa.SalesAreaKey
LEFT JOIN [Customer Contact] cc ON aa.CustomerContactKey = cc.[Customer Contact Key]
LEFT JOIN [DM Due Date] dd ON aa.DMDateKey = dd.[DM Due Date Key]
LEFT JOIN Invoicing i ON aa.InvoicingKey = i.[Invoicing Key]
WHERE ((a.[Fee Type] = 'Rights') 
	OR (a.[Fee Type] = 'Technical' AND p.OmitException = 1)) --Ticket #161 Omit all technical allocations from the IMG Replay Sales report with the exception..
	AND mf.[Actual Amount USD Filtered] IS NOT NULL</CommandText>
