  <DataSets>
    <DataSet Name="AviabilitiesReport">
      <Query>
        <DataSourceName>MediaDM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDealRightsLicenseStart">
            <Value>=Parameters!FromDealRightsLicenseStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDealRightsLicenseEnd">
            <Value>=Parameters!ToDealRightsLicenseEnd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Property">
            <Value>=Parameters!Property.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PropertyRights">
            <Value>=Parameters!PropertyRights.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Territory">
            <Value>=Parameters!Territory.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @LicenseStart DATE = PARSE(@FromDealRightsLicenseStart AS date USING 'en-US')
DECLARE @dateTo DATE = 
CASE 
	WHEN (@ToDealRightsLicenseEnd = '' OR @ToDealRightsLicenseEnd IS NULL  ) THEN '9999-12-31' 
	ELSE PARSE(@ToDealRightsLicenseEnd AS date USING 'en-US')
END


SELECT 
	MIN([License Start]) OVER () AS [License Start Min]
	,MAX([License End]) OVER () AS [License End Max]
	,DATEDIFF(DAY, 0 , [License Start]) AS [License Start Int]
	,COUNT(*) OVER() AS CountOfItems
	,ROW_NUMBER() OVER (ORDER BY [Property Name], [Rights Selection], [Is Deal Rights], [License Start]) AS SortingKey
	,CASE WHEN LEN([Property Name]) &gt; 50 THEN
			SUBSTRING([Property Name], 0, 50) + ' ...'
		ELSE 
			SUBSTRING([Property Name], 0, 50)
	END AS [Property Name Short]
	,CASE WHEN LEN([Rights Selection]) &gt; 50 THEN
			SUBSTRING([Rights Selection], 0, 50) + ' ...'
		ELSE 
			SUBSTRING([Rights Selection], 0, 50)
	END AS [Rights Selection Short]
	,CASE WHEN LEN([Deal Property Name]) &gt; 50 THEN
			SUBSTRING([Deal Property Name], 0, 50) + ' ...'
		ELSE 
			SUBSTRING([Deal Property Name], 0, 50)
	END AS [Deal Property Name Short]
	,t.[Customer Short Name]
	,t.GroupingKey
	,t.[Is Deal Rights]
	,t.[Language Description]
	,t.[Number of Transmissions]
	,t.[License Start]
	,t.[License End]
	,t.[Rights Selection]
	,t.[Deal Property Name]
,[Property Name]
	,t.CountRights
FROM
(
	--- Properties
	SELECT 
		MIN(dr.[License Start]) AS [License Start]
		,MAX(dr.[License End]) AS [License End]
		,' ' AS [Customer Short Name]
		,p.[Property Name] 
		,pr.[Rights Selection]
		,'' AS [Deal Property Name] 
		,CAST(HASHBYTES('SHA1',p.[Property Name] + pr.[Rights Selection]) AS BIGINT) AS [GroupingKey]
		,'' AS [Language Description]
		,0 AS [License Duration]
		,0 AS [Number of Transmissions]
		,COUNT(r.[Deal Rights Key]) AS CountRights 
		,p.[Property Key] AS PropertyId
		,0 AS [Is Deal Rights]
	FROM [dbo].[Deal Rights] dr
	INNER JOIN [dbo].[Rights] r ON dr.[Deal Rights Key] = r.[Deal Rights Key]
	INNER JOIN [dbo].[Property] p ON r.[Property Key] = p.[Property Key]
	INNER JOIN [dbo].[Customer Contact] cc ON r.[Customer Contact Key] = cc.[Customer Contact Key]
	INNER JOIN [dbo].[Property Rights] pr ON r.[Property Rights Key] = pr.[Property Rights Key]
	WHERE 0=0
		AND p.[Property Key] IN (@Property)
		AND dr.[License Start] BETWEEN @LicenseStart AND @dateTo
		AND (pr.[Property Rights Key] IN (@PropertyRights) OR  (NULL IN (@PropertyRights)))
		AND (dr.[Territory Description] IN (@Territory) OR (NULL IN (@Territory)))
	GROUP BY  
		p.[Property Key],
		p.[Property Name],
		pr.[Rights Selection]

	UNION ALL

	-- deal rights
	SELECT 
		dr.[License Start]
		,dr.[License End]
		,cc.[Customer Short Name]
		,p.[Property Name]
		,pr.[Rights Selection]  
		,dr.[Deal Rights Description] AS [Deal Property Name] 
		,CAST(HASHBYTES('SHA1',dr.[Territory Description] + pr.[Rights Selection] + [Deal Rights Description]) AS BIGINT) AS [GroupingKey]
		,dr.[Language Description]
		,dr.[License Duration]
		,dr.[Number of Transmissions]
		,COUNT(r.[Deal Rights Key]) AS CountRights 
		,p.[Property Key] AS PropertyId
		,1 AS [Is Deal Rights]
	FROM [dbo].[Deal Rights] dr
	INNER JOIN [dbo].[Rights] r ON dr.[Deal Rights Key] = r.[Deal Rights Key]
	INNER JOIN [dbo].[Property] p ON r.[Property Key] = p.[Property Key]
	INNER JOIN [dbo].[Customer Contact] cc ON r.[Customer Contact Key] = cc.[Customer Contact Key]
	INNER JOIN [dbo].[Property Rights] pr ON r.[Property Rights Key] = pr.[Property Rights Key]
	WHERE 0=0
		AND p.[Property Key] IN (@Property)
		AND dr.[License Start] BETWEEN @LicenseStart AND @dateTo
		AND (pr.[Property Rights Key] IN (@PropertyRights) OR  (NULL IN (@PropertyRights)))
		AND (dr.[Territory Description] IN (@Territory) OR (NULL IN (@Territory)))
	GROUP BY  
		p.[Property Key],
		p.[Property Name],
		pr.[Rights Selection],
		dr.[License Start],
		dr.[License End],
		cc.[Customer Short Name], 
		dr.[Deal Rights Description], 
		dr.[Territory Description],
		dr.[Language Description],
		dr.[License Duration], 
		dr.[Number of Transmissions]
) AS t</CommandText>
