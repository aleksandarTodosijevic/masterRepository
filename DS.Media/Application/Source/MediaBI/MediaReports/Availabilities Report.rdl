        <CommandText>SELECT
[Property Key]
,[Property Name]
,[Short Name (Property)] AS [Short Name]
FROM [dbo].[Property] p
WHERE 
(UPPER([Property Name]) like UPPER('%' + @PropertyNameContains + '%') OR @PropertyNameContains IS NULL)
AND (([Property Category]  IN ('Standard', 'Series Header')  AND @Standard_Header_Properties = 1) OR (@Standard_Header_Properties = 0))</CommandText>
      </Query>
