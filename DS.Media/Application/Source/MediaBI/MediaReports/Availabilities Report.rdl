            <Value>=Parameters!Property.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @LicenseStart DATE = PARSE(@FromDealRightsLicenseStart AS date USING 'en-US')
DECLARE @dateTo DATE = 
CASE 
	WHEN (@ToDealRightsLicenseEnd = '' OR @ToDealRightsLicenseEnd IS NULL  ) THEN '9999-12-31' 
	ELSE PARSE(@ToDealRightsLicenseEnd AS date USING 'en-US')
END
SELECT 
		p.[Property Name] 
		,pr.[Rights Selection]
		,pr.[Property Rights Key]
		,COUNT(dr.[Deal Rights Key]) AS CountRights
	FROM [dbo].[Deal Rights] dr
	INNER JOIN [dbo].[Rights] r ON dr.[Deal Rights Key] = r.[Deal Rights Key]
	INNER JOIN [dbo].[Property] p ON r.[Property Key] = p.[Property Key]
	INNER JOIN [dbo].[Customer Contact] cc ON r.[Customer Contact Key] = cc.[Customer Contact Key]
	INNER JOIN [dbo].[Property Rights] pr ON r.[Property Rights Key] = pr.[Property Rights Key]
	WHERE 0=0
		AND p.[Property Key] IN (@Property)
		AND dr.[License Start] between @LicenseStart AND @dateTo
	GROUP BY 		p.[Property Name] 
		,pr.[Rights Selection]
		,pr.[Property Rights Key]</CommandText>
      </Query>
      <Fields>
        <Field Name="Property_Name">
          <DataField>Property Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Rights_Selection">
          <DataField>Rights Selection</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Property_Rights_Key">
          <DataField>Property Rights Key</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="CountRights">
          <DataField>CountRights</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Territory">
      <Query>
        <DataSourceName>MediaDM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DealRightsTerritoryDescription">
            <Value>=Parameters!DealRightsTerritoryDescription.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FromDealRightsLicenseStart">
            <Value>=Parameters!FromDealRightsLicenseStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDealRightsLicenseEnd">
            <Value>=Parameters!ToDealRightsLicenseEnd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Property">
            <Value>=Parameters!Property.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PropertyRights">
            <Value>=Parameters!PropertyRights.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @territory varchar(100) = @DealRightsTerritoryDescription

DECLARE @LicenseStart DATE = PARSE(@FromDealRightsLicenseStart AS date USING 'en-US')
DECLARE @dateTo DATE = 
CASE 
	WHEN (@ToDealRightsLicenseEnd = '' OR @ToDealRightsLicenseEnd IS NULL  ) THEN '9999-12-31' 
	ELSE PARSE(@ToDealRightsLicenseEnd AS date USING 'en-US')
END


SELECT 
	Distinct [Territory Description] AS [TerritoryDescription]
FROM [dbo].[Deal Rights] dr
	INNER JOIN [dbo].[Rights] r ON dr.[Deal Rights Key] = r.[Deal Rights Key]
	INNER JOIN [dbo].[Property] p ON r.[Property Key] = p.[Property Key]
	INNER JOIN [dbo].[Customer Contact] cc ON r.[Customer Contact Key] = cc.[Customer Contact Key]
	INNER JOIN [dbo].[Property Rights] pr ON r.[Property Rights Key] = pr.[Property Rights Key]
WHERE 
	p.[Property Key] IN (@Property)
	AND dr.[License Start] between @LicenseStart AND @dateTo
	AND (pr.[Property Rights Key] IN (@PropertyRights) OR  (NULL IN (@PropertyRights)))
	AND (UPPER([Territory Description]) like UPPER('%' + @territory +'%') OR (@territory IS NULL))</CommandText>
      </Query>
      <Fields>
        <Field Name="TerritoryDescription">
          <DataField>TerritoryDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
