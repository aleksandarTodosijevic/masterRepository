    <DataSet Name="PropertyRights">
      <Query>
        <DataSourceName>MediaDM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDealRightsLicenseStart">
            <Value>=Parameters!FromDealRightsLicenseStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDealRightsLicenseEnd">
